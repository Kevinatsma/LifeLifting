{
  "name": "life-lifting",
  "version": "1.0.0",
  "scripts": {
    "ng": "ng",
    "nghm": "node --max_old_space_size=8096 ./node_modules/@angular/cli/bin/ng",
    "start": "ng serve",
    "build": "ng build",
    "test": "ng test",
    "lint": "ng lint",
    "e2e": "ng e2e",
    "dev": "npm run build:client-and-server-bundles && node dist/server/main.js",
    "build-prod": "bash ./scripts/ng.sh build --prod --aot",
    "compile:server_bak": "webpack --config webpack.server.config.js --progress --colors",
    "build:client-and-server-bundles": "ng build --prod && gzip-all \"dist/browser/*.*\" && ng run life-lifting:server:production --bundleDependencies",
    "build:client-and-server-bundles_bak": "npm run build-prod && npm run nghm run life-lifting:server:production",
    "dev:ssr": "ng run life-lifting:serve-ssr",
    "serve:ssr": "node dist/server/main.js",
    "build:ssr": "ng build --prod && ng run life-lifting:server:production",
    "prerender": "ng run life-lifting:prerender"
  },
  "private": true,
  "dependencies": {
    "@angular/animations": "^10.1.0",
    "@angular/cdk": "^10.2.0",
    "@angular/common": "^10.1.0",
    "@angular/compiler": "^10.1.0",
    "@angular/core": "^10.1.0",
    "@angular/fire": "^6.0.2",
    "@angular/forms": "^10.1.0",
    "@angular/material": "^10.2.0",
    "@angular/platform-browser": "^10.1.0",
    "@angular/platform-browser-dynamic": "^10.1.0",
    "@angular/platform-server": "^10.1.0",
    "@angular/pwa": "^0.803.1",
    "@angular/router": "^10.1.0",
    "@angular/service-worker": "^10.1.0",
    "@nguniversal/express-engine": "^10.1.0",
    "@types/jspdf": "^1.3.1",
    "angularx-flatpickr": "^6.1.0",
    "core-js": "^3.2.1",
    "express": "^4.17.1",
    "firebase": "^7.19.1",
    "flatpickr": "^4.6.2",
    "gzip-all": "^1.0.0",
    "jspdf": "^2.1.0",
    "moment": "^2.24.0",
    "node-sass": "^4.12.0",
    "rxjs": "^6.5.2",
    "typescript": "~3.9.7",
    "zone.js": "^0.10.3"
  },
  "devDependencies": {
    "@angular-devkit/build-angular": "^0.1001.0",
    "@angular/cli": "^10.1.0",
    "@angular/compiler-cli": "^10.1.0",
    "@angular/language-service": "^10.1.0",
    "@fortawesome/fontawesome-free": "^5.10.2",
    "@nguniversal/builders": "^10.1.0",
    "@types/express": "^4.17.0",
    "@types/jasmine": "^3.4.0",
    "@types/jasminewd2": "~2.0.3",
    "@types/node": "^12.7.2",
    "angular-calendar": "^0.28.0-rc.0",
    "calendar-utils": "^0.3.0-beta.8",
    "codelyzer": "~5.1.0",
    "date-fns": "^2.0.1",
    "html2canvas": "^1.0.0-rc.1",
    "jasmine-core": "~3.4.0",
    "jasmine-spec-reporter": "~4.2.1",
    "karma": "^4.3.0",
    "karma-chrome-launcher": "~3.1.0",
    "karma-coverage-istanbul-reporter": "~2.1.0",
    "karma-jasmine": "~2.0.1",
    "karma-jasmine-html-reporter": "^1.4.2",
    "protractor": "^5.4.2",
    "ts-loader": "^6.0.4",
    "ts-node": "~8.3.0",
    "tslint": "~5.19.0",
    "webpack-cli": "^3.3.7"
  }
}
